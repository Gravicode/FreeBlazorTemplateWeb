                    <img class="card-img-top" src="https://source.unsplash.com/TIutDBFEtcY/700x500" alt="..." />
                    <div class="card-body"><div class="card-title">Book Stand</div></div>
                </a>
            </div>
        </div>
    </div>
</section>
<section class="bg-white py-10">
    <div class="container px-5">
        <div class="row gx-5 justify-content-center">
            <div class="col-xl-6 col-lg-8 col-md-10 text-center py-5">
                <h2>Ready to get Started?</h2>
                <p class="lead text-gray-500 mb-5">We'd love to hear from you about your branding and digital needs. We are currently accepting new clients.</p>
                <a class="btn btn-primary fw-500" href="#!">Contact us</a>
            </div>
        </div>
    </div>
    <div class="svg-border-rounded text-dark">
        <!-- Rounded SVG Border-->
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 144.54 17.34" preserveAspectRatio="none" fill="currentColor"><path d="M144.54,17.34H0V0H144.54ZM0,0S32.36,17.34,72.27,17.34,144.54,0,144.54,0"></path></svg>
    </div>
</section>
@code {
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {

            await JS.InvokeVoidAsync("LoadFeather");
        }
    }
}
